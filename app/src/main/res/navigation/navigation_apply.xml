<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_apply"
    app:startDestination="@id/applyPersonalFragment">

    <fragment
        android:id="@+id/applyFirstFragment"
        android:name="com.example.ceria.ui.home.apply.questionnaire.ApplyFirstFragment"
        android:label="fragment_apply_first"
        tools:layout="@layout/fragment_apply_first">
        <action
            android:id="@+id/action_applyFirstFragment_to_applySecondFragment"
            app:destination="@id/applySecondFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/applySecondFragment"
        android:name="com.example.ceria.ui.home.apply.questionnaire.ApplySecondFragment"
        android:label="ApplySecondFragment"
        tools:layout="@layout/fragment_apply_second">
        <argument
            android:name="isPayroll"
            app:argType="boolean" />
        <action
            android:id="@+id/action_applySecondFragment_to_applyThirdFragment"
            app:destination="@id/applyThirdFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/applyThirdFragment"
        android:name="com.example.ceria.ui.home.apply.initial_form.ApplyThirdFragment"
        android:label="ApplyThirdFragment"
        tools:layout="@layout/fragment_apply_third">
        <action
            android:id="@+id/action_applyThirdFragment_to_applyFourthFragment"
            app:destination="@id/applyFourthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_applyThirdFragment_to_nav_graph"
            app:destination="@id/nav_main"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/applyFourthFragment"
        android:name="com.example.ceria.ui.home.apply.main_form.ApplyFourthFragment"
        android:label="ApplyFourthFragment"
        tools:layout="@layout/fragment_apply_fourth">
        <action
            android:id="@+id/action_applyFourthFragment_to_applyInputKtpFragment"
            app:destination="@id/applyInputKtpFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_applyFourthFragment_to_applyPersonalFragment"
            app:destination="@id/applyPersonalFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_applyFourthFragment_to_applyFamilyFragment"
            app:destination="@id/applyFamilyFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_applyFourthFragment_to_applyJobFragment"
            app:destination="@id/applyJobFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_applyFourthFragment_to_applyScoringFragment"
            app:destination="@id/applyScoringFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_applyFourthFragment_to_applyIncomeUploadFragment"
            app:destination="@id/applyIncomeUploadFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/applyPersonalFragment"
        android:name="com.example.ceria.ui.home.apply.main_form.ApplyPersonalFragment"
        android:label="ApplyPersonalFragment"
        tools:layout="@layout/fragment_apply_personal">
        <action
            android:id="@+id/action_applyPersonalFragment_to_applyFourthFragment"
            app:destination="@id/applyFourthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/applyInputKtpFragment"
        android:name="com.example.ceria.ui.home.apply.main_form.ApplyInputKtpFragment"
        android:label="fragment_apply_input_ktp"
        tools:layout="@layout/fragment_apply_input_ktp">
        <action
            android:id="@+id/action_applyInputKtpFragment_to_applyFourthFragment"
            app:destination="@id/applyFourthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/applyFamilyFragment"
        android:name="com.example.ceria.ui.home.apply.main_form.ApplyFamilyFragment"
        android:label="ApplyFamilyFragment"
        tools:layout="@layout/fragment_apply_family">
        <action
            android:id="@+id/action_applyFamilyFragment_to_applyFourthFragment"
            app:destination="@id/applyFourthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/applyJobFragment"
        android:name="com.example.ceria.ui.home.apply.main_form.ApplyJobFragment"
        android:label="ApplyJobFragment"
        tools:layout="@layout/fragment_apply_job">
        <action
            android:id="@+id/action_applyJobFragment_to_applyFourthFragment"
            app:destination="@id/applyFourthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/applyScoringFragment"
        android:name="com.example.ceria.ui.home.apply.ApplyScoringFragment"
        android:label="ApplyScoringFragment"
        tools:layout="@layout/fragment_apply_scoring">
        <action
            android:id="@+id/action_applyScoringFragment_to_nav_graph"
            app:destination="@id/nav_main"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/applyScoringFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/applyIncomeUploadFragment"
        android:name="com.example.ceria.ui.home.apply.main_form.ApplyIncomeUploadFragment"
        android:label="ApplyIncomeUploadFragment"
        tools:layout="@layout/fragment_apply_income_upload">
        <action
            android:id="@+id/action_applyIncomeUploadFragment_to_applyScoringFragment"
            app:destination="@id/applyScoringFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_applyIncomeUploadFragment_to_applyInputIncomeFragment"
            app:destination="@id/applyInputIncomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/applyInputIncomeFragment"
        android:name="com.example.ceria.ui.home.apply.main_form.ApplyInputIncomeFragment"
        android:label="ApplyInputIncomeFragment"
        tools:layout="@layout/fragment_apply_input_income">
        <action
            android:id="@+id/action_applyInputIncomeFragment_to_applyIncomeUploadFragment"
            app:destination="@id/applyIncomeUploadFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
</navigation>