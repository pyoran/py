<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_finalize"
    app:startDestination="@id/finalizeFirstFragment">

    <fragment
        android:id="@+id/finalizeFirstFragment"
        android:name="com.example.ceria.ui.home.finalize.initial_page.FinalizeFirstFragment"
        android:label="FinalizeFirstFragment"
        tools:layout="@layout/fragment_finalize_first">
        <action
            android:id="@+id/action_finalizeFirstFragment_to_finalizeSecondFragment"
            app:destination="@id/finalizeSecondFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/finalizeSecondFragment"
        android:name="com.example.ceria.ui.home.finalize.initial_page.FinalizeSecondFragment"
        android:label="FinalizeSecondFragment"
        tools:layout="@layout/fragment_finalize_second">
        <action
            android:id="@+id/action_finalizeSecondFragment_to_finalizeThirdFragment"
            app:destination="@id/finalizeThirdFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/finalizeThirdFragment"
        android:name="com.example.ceria.ui.home.finalize.digital_signature.FinalizeThirdFragment"
        android:label="FinalizeThirdFragment"
        tools:layout="@layout/fragment_finalize_third">
        <action
            android:id="@+id/action_finalizeThirdFragment_to_finalizeFourthFragment"
            app:destination="@id/finalizeFourthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_finalizeThirdFragment_to_finalizeSelfieFragment"
            app:destination="@id/finalizeSelfieFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/finalizeFourthFragment"
        android:name="com.example.ceria.ui.home.finalize.agreement.FinalizeFourthFragment"
        android:label="FinalizeFourthFragment"
        tools:layout="@layout/fragment_finalize_fourth">
        <action
            android:id="@+id/action_finalizeFourthFragment_to_finalizeFifthFragment"
            app:destination="@id/finalizeFifthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/finalizeFifthFragment"
        android:name="com.example.ceria.ui.home.finalize.security.FinalizeFifthFragment"
        android:label="FinalizeFifthFragment"
        tools:layout="@layout/fragment_finalize_fifth">
        <action
            android:id="@+id/action_finalizeFifthFragment_to_finalizeSixthFragment"
            app:destination="@id/finalizeSixthFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/finalizeFinishedFragment"
        android:name="com.example.ceria.ui.home.finalize.finished.FinalizeFinishedFragment"
        android:label="FinalizeFinishedFragment"
        tools:layout="@layout/fragment_finalize_finished">
        <action
            android:id="@+id/action_finalizeFinishedFragment_to_finalizeWelcomePackageFragment"
            app:destination="@id/finalizeWelcomePackageFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_finalizeFinishedFragment_to_nav_graph"
            app:destination="@id/nav_main"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/finalizeFinishedFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/finalizeHelpFragment"
        android:name="com.example.ceria.ui.home.finalize.finished.FinalizeHelpFragment"
        android:label="FinalizeHelpFragment"
        tools:layout="@layout/fragment_finalize_help" />
    <fragment
        android:id="@+id/finalizeSelfieFragment"
        android:name="com.example.ceria.ui.home.finalize.digital_signature.FinalizeSelfieFragment"
        android:label="FinalizeSelfieFragment"
        tools:layout="@layout/fragment_finalize_selfie">
        <action
            android:id="@+id/action_finalizeSelfieFragment_to_finalizeThirdFragment"
            app:destination="@id/finalizeThirdFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/finalizeSixthFragment"
        android:name="com.example.ceria.ui.home.finalize.security.FinalizeSixthFragment"
        android:label="FinalizeSixthFragment"
        tools:layout="@layout/fragment_finalize_sixth">
        <action
            android:id="@+id/action_finalizeSixthFragment_to_finalizeHelpFragment"
            app:destination="@id/finalizeHelpFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_finalizeSixthFragment_to_finalizeFinishedFragment"
            app:destination="@id/finalizeFinishedFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/finalizeWelcomePackageFragment"
        android:name="com.example.ceria.ui.home.finalize.finished.FinalizeWelcomePackageFragment"
        android:label="FinalizeWelcomePackageFragment"
        tools:layout="@layout/fragment_finalize_welcome_package" />
</navigation>